<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- puerta.system.po.OrderNoLog root -->
	<class name="puerta.system.po.OrderNoLog" table="ordernolog" dynamic-update="true">
		<id name="id" type="string">
			<column name="id" length="36"/>
			<generator class="uuid2"/>
		</id>
		<property name="date" column="date" type="calendar"/>
		<property name="logTime" column="logTime" type="calendar"/>
		<property name="counter" column="count" type="int"/>
		<property name="no" column="no" type="string" length="40"/>
	</class>
</hibernate-mapping>
