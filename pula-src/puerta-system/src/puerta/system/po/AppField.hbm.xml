<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- puerta.system.po.AppField root -->
	<class name="puerta.system.po.AppField" table="appfield">
		<id name="id" type="string">
			<column name="id" length="36"/>
			<generator class="uuid2"/>
		</id>
		<property name="name" column="name" type="string" length="40"/>
		<property name="removed" column="removed" type="boolean"/>
		<property name="path" column="path" type="string" length="40"/>
		<property name="no" column="no" type="string" length="20"/>
		<property name="comments" length="200"/>
		<property name="indexNo"/>
	</class>
</hibernate-mapping>
