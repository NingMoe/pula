<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<!-- puerta.system.po.RequestUri root -->
	<class name="puerta.system.po.RequestUri" table="requesturi">
		<id name="id" type="string">
			<column name="id" length="36"/>
			<generator class="uuid2"/>
		</id>
		<property name="uri" column="uri" type="string" length="120"/>
		<property name="assignedCount" column="assignedCount" type="int"/>
		<property name="onlineCheck"/>
		<property name="purviewCheck"/>
	</class>
</hibernate-mapping>
