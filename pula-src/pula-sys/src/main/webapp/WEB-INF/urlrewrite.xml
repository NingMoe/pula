<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite use-query-string="false">

	<!-- Spring Surf -->
	<!-- <rule>
		<from>/proxy/(.*)</from>
		<to>/service/proxy/$1</to>
	</rule>
	<rule>
		<from>/res/(.*)</from>
		<to>/service/resource/$1</to>
	</rule>		 -->
	<!-- <rule>
		<from>/index.html</from>
		<to>/html/index.html</to>
	</rule> -->
	<rule>
		<from>/favicon.ico</from>
		<to>/favicon.ico</to>
	</rule>
	<rule>
		<from>/barcode</from>
		<to>/barcode</to>
	</rule>
	<rule>
		<from>/service/(.*)</from>
		<to>/service/$1</to>
	</rule>
	<rule>
		<from>/services/(.*)</from>
		<to>/services/$1</to>
	</rule>
	<rule>
		<from>/xmlrpc</from>
		<to>/xmlrpc</to>
	</rule>	
	<rule>
		<from>/static/(.*)</from>
		<to>/static/$1</to>
	</rule>
	<rule>
		<from>/fckeditor/editor/filemanager/(.*)</from>
		<to>/fckeditor/editor/filemanager/$1</to>
	</rule>
	<rule>
		<from>/fckeditor/(.*)</from>
		<to>/fckeditor/$1</to>
	</rule>
	<rule>
		<from>/files/(.*)</from>
		<to>/files/$1</to>
	</rule>
	<rule>
		<from>/userfiles/(.*)</from>
		<to>/userfiles/$1</to>
	</rule>
	<rule>
		<from>/(.*)jpg</from>
		<to>/$1jpg</to>
	</rule>
	<rule>
		<from>/(.*)</from>
		<to>/service/$1</to>
	</rule>

	<outbound-rule>
		<from>/service/(.*)</from>
		<to>/$1</to>
	</outbound-rule>

</urlrewrite>