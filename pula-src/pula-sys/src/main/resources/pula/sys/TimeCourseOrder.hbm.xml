<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- pula.sys.domains.Course root -->
 <class name="pula.sys.domains.TimeCourseOrder" table="time_course_order">
  <id column="id" name="id" type="long">
   <generator class="native"/>
  </id>
  <property column="comments" generated="never" lazy="false"
   length="200" name="comments" type="string"/>
  <property column="removed" generated="never" lazy="false"
   name="removed" type="boolean"/>
  <property column="enabled" generated="never" lazy="false"
   name="enabled" type="boolean"/>
  <property column="no" generated="never" lazy="false" length="40"
   name="no" type="string"/>
  <property column="createTime" generated="never" lazy="false"
   name="createTime" type="date"/>
  <property column="updateTime" generated="never" lazy="false"
   name="updateTime" type="date"/>
  <property column="courseNo" generated="never" lazy="false"
   name="courseNo" type="string"/>
  <property column="studentNo" generated="never" lazy="false"
   name="studentNo" type="string"/>
  <property column="buyType" generated="never" lazy="false"
   name="buyType" type="int"/>
  <property column="paied" generated="never" lazy="false" name="paied" type="int"/>
  <property column="paiedCount" generated="never" lazy="false"
   name="paiedCount" type="int"/>
  <property column="creator" generated="never" lazy="false"
   name="creator" type="string"/>
  <property column="updator" generated="never" lazy="false"
   name="updator" type="string"/>
  <property column="remainCount" generated="never" lazy="false"
   name="usedCount" type="int"/>
  <property column="remainCost" generated="never" lazy="false"
   name="usedCost" type="int"/>
  <property column="gongfangCount" generated="never" lazy="false"
   name="gongfangCount" type="int"/>
  <property column="usedGongfangCount" generated="never" lazy="false"
   name="usedGongFangCount" type="int"/>
  <property column="huodongCount" generated="never" lazy="false"
   name="huodongCount" type="int"/>
  <property column="usedHuodongCount" generated="never" lazy="false"
   name="usedHuodongCount" type="int"/>
  <property column="orderStatus" generated="never" lazy="false"
   name="orderStatus" type="int"/>
  <property column="courseTime" generated="never" lazy="false"
   name="courseTime" type="string"/>
  <property column="usedSpecialCourseCount" generated="never"
   lazy="false" name="usedSpecialCourseCount" type="int"/>
  <property column="specialCourseCount" generated="never" lazy="false"
   name="specialCourseCount" type="int"/>
  <property column="level" name="level" type="string"/>
  
   <!-- wechat notify entity -->
   <property column="return_code" generated="never" lazy="false"
   name="return_code" type="string"/>
   
   <property column="return_msg" generated="never" lazy="false"
   name="return_msg" type="string"/>
   <property column="appid" generated="never" lazy="false"
   name="appid" type="string"/>
   <property column="mch_id" generated="never" lazy="false"
   name="mch_id" type="string"/>
   <property column="device_info" generated="never" lazy="false"
   name="device_info" type="string"/>
   <property column="nonce_str" generated="never" lazy="false"
   name="nonce_str" type="string"/>
   <property column="sign" generated="never" lazy="false"
   name="sign" type="string"/>
   <property column="result_code" generated="never" lazy="false"
   name="result_code" type="string"/>
   <property column="err_code" generated="never" lazy="false"
   name="err_code" type="string"/>
   <property column="err_code_des" generated="never" lazy="false"
   name="err_code_des" type="string"/>
   <property column="openid" generated="never" lazy="false"
   name="openid" type="string"/>
   <property column="is_subscribe" generated="never" lazy="false"
   name="is_subscribe" type="string"/>
   <property column="trade_type" generated="never" lazy="false"
   name="trade_type" type="string"/>
   <property column="bank_type" generated="never" lazy="false"
   name="bank_type" type="string"/>
   <property column="fee_type" generated="never" lazy="false"
   name="fee_type" type="string"/>
   
   <property column="cash_fee_type" generated="never" lazy="false"
   name="cash_fee_type" type="string"/>
   <property column="transaction_id" generated="never" lazy="false"
   name="transaction_id" type="string"/>
   <property column="out_trade_no" generated="never" lazy="false"
   name="out_trade_no" type="string"/>
   
   <property column="attach" generated="never" lazy="false"
   name="attach" type="string"/>
   <property column="time_end" generated="never" lazy="false"
   name="time_end" type="string"/>
   
   <!-- int -->
   <property column="total_fee" generated="never" lazy="false"
   name="total_fee" type="int"/>
   <property column="cash_fee" generated="never" lazy="false"
   name="cash_fee" type="int"/>
   <property column="coupon_fee" generated="never" lazy="false"
   name="coupon_fee" type="int"/>
   <property column="coupon_count" generated="never" lazy="false"
   name="coupon_count" type="int"/>
  
 </class>
</hibernate-mapping>
