<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <!-- pula.sys.domains.Course root -->
 <class name="pula.sys.domains.NoticeOrder" table="notice_order">
  <id column="id" name="id" type="long">
   <generator class="native"/>
  </id>
  <property column="comments" generated="never" lazy="false"
   length="200" name="comments" type="string"/>
  <property column="removed" generated="never" lazy="false"
   name="removed" type="boolean"/>
  <property column="enabled" generated="never" lazy="false"
   name="enabled" type="boolean"/>
  <property column="createTime" generated="never" lazy="false"
   name="createTime" type="date"/>
  <property column="updateTime" generated="never" lazy="false"
   name="updateTime" type="date"/>
  <property column="no" generated="never" lazy="false" name="no" type="string"/>
  <property column="noticePrice" generated="never" lazy="false"
   name="noticePrice" type="long"/>
  <property column="noticeId" generated="never" lazy="false"
   name="noticeId" type="long"/>
  <property column="noticeNo" generated="never" lazy="false"
   name="noticeNo" type="string"/>
  <property column="noticeName" generated="never" lazy="false"
   name="noticeName" type="string"/>
  <property column="count" generated="never" lazy="false" name="count" type="int"/>
  <property column="studentNo" generated="never" lazy="false"
   name="studentNo" type="string"/>
  <property column="studentId" generated="never" lazy="false"
   name="studentId" type="long"/>
  <property column="orderPayStatus" generated="never" lazy="false"
   name="orderPayStatus" type="int"/>
  <property column="paied" generated="never" lazy="false" name="paied" type="double"/>
  <property column="prepayId" generated="never" lazy="false"
   name="prepayId" type="string"/>
  <property column="accessToken" generated="never" lazy="false"
   name="accessToken" type="string"/>
  <property column="wxOrderId" generated="never" lazy="false"
   name="wxOrderId" type="string"/>
  <property column="wxOrderDetail" generated="never" lazy="false"
   name="wxOrderDetail" type="string"/>
 </class>
</hibernate-mapping>
